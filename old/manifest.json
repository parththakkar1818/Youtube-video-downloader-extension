{
    "manifest_version": 3,
    "name": "YouTube Downloader",
    "version": "1.0",
    "permissions": [
      "activeTab",
      "scripting",
      "downloads"
    ],
    "action": {
      "default_popup": "index.html"
    },
    "content_scripts": [{
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"]
    }]
  }
  